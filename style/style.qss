QWidget[exp_style="true"]{
	margin-top: 0px;
	margin-left: 5px;
}
QLineEdit[exp_style="true"]{
  background-color: rgba(0, 0, 0, 0);
  border-radius: 15px;
  /*border: 0px;*/
  border: 2px solid #e0e4e7;
  /*padding: 0px 15px;*/
