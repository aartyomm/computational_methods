  color: #A04D73;
  font-size: 12px;
  font-weight: normal;
}

QLabel[exp_style="true"][heading = "true"]{
  font-size: 24px;
}
QWidget[exp_style="true"][subheading = "true"]{
  /*font-size: 18px;*/
  font: 75 24px "Century Gothic";
  font-weight: bold;
}

